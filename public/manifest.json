{
  "name": "Quick Bookmark",
  "description": "Quickly bookmark or open existing bookmarks",
  "version": "2.1.0",
  "manifest_version": 3,
  "permissions": ["bookmarks", "tabs", "tabGroups", "storage"],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "commands": {
    "add": {
      "suggested_key": {
        "default": "Ctrl+D"
      },
      "description": "Open Quick Bookmark popup"
    },
    "open": {
      "suggested_key": {
        "default": "Alt+F"
      },
      "description": "Open Quick Open search to find existing bookmarks"
    }
  }
}
