{
  "name": "Quick Bookmark",
  "description": "Quickly bookmark the current page into a folder via fuzzy search.",
  "version": "1.0.0",
  "manifest_version": 3,
  "permissions": [
    "bookmarks",
    "tabs"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+D"
      },
      "description": "Open Quick Bookmark popup"
    }
  }
}
